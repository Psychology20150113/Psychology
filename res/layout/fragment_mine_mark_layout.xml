<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@color/white"
    android:gravity="center_horizontal">
    <com.github.mikephil.charting.charts.RadarChart
        android:id="@+id/view_chart"
        android:layout_width="230dp"
        android:layout_height="230dp"/>
    <TextView 
        android:id="@+id/tv_zhiye"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        style="@style/text_black16_style"/>
    <TextView 
        android:id="@+id/tv_empty_zhiye"
        style="@style/text_black18_style"
        android:text="@string/empty_zhiye"
        android:visibility="gone"/>
	<View 
	    android:layout_width="match_parent"
	    android:layout_height="10dp"
	    android:background="@color/v2_divider"/>
	<LinearLayout 
	    android:id="@+id/ll_qizhi_show"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
	    android:gravity="center_horizontal">
		<LinearLayout 
		    android:layout_width="wrap_content"
		    android:layout_height="20dp"
		    android:layout_marginTop="15dp"
		    android:gravity="center_vertical">
		    <TextView 
		        style="@style/text_gray16_style"
		        android:textSize="14sp"
		        android:text="@string/qizhi_one"/>
		    <LinearLayout 
		        android:layout_width="200dp"
		        android:layout_height="match_parent"
		        android:layout_marginLeft="5dp"
		        android:gravity="center_vertical"
		        android:background="@drawable/shape_gray_rect">
				<ProgressBar 
				    android:id="@+id/pb_qizhi_one"
			        android:layout_width="match_parent"
			        android:layout_height="7dp"
			        android:progress="50"
			        style="@android:style/Widget.ProgressBar.Horizontal"
			        android:progressDrawable="@drawable/bg_progress_bar"/>	        
		    </LinearLayout>
			<TextView 
			    android:id="@+id/tv_point_one"
			    style="@style/text_gray16_style"
			    android:textSize="14sp"/>
		</LinearLayout>
		<LinearLayout 
		    android:layout_width="wrap_content"
		    android:layout_height="20dp"
		    android:layout_marginTop="10dp"
		    android:gravity="center_vertical">
		    <TextView 
		        style="@style/text_gray16_style"
		        android:textSize="14sp"
		        android:text="@string/qizhi_two"/>
		    <LinearLayout 
		        android:layout_width="200dp"
		        android:layout_height="match_parent"
		        android:layout_marginLeft="5dp"
		        android:gravity="center_vertical"
		        android:background="@drawable/shape_gray_rect">
				<ProgressBar 
				    android:id="@+id/pb_qizhi_two"
			        android:layout_width="match_parent"
			        android:layout_height="7dp"
			        android:progress="50"
			        style="@android:style/Widget.ProgressBar.Horizontal"
			        android:progressDrawable="@drawable/bg_progress_bar"/>	        
		    </LinearLayout>
			<TextView 
			    android:id="@+id/tv_point_two"
			    style="@style/text_gray16_style"
			    android:textSize="14sp"/>
		</LinearLayout>
		<LinearLayout 
		    android:layout_width="wrap_content"
		    android:layout_height="20dp"
		    android:layout_marginTop="10dp"
		    android:gravity="center_vertical">
		    <TextView 
		        style="@style/text_gray16_style"
		        android:textSize="14sp"
		        android:text="@string/qizhi_three"/>
		    <LinearLayout 
		        android:layout_width="200dp"
		        android:layout_height="match_parent"
		        android:layout_marginLeft="5dp"
		        android:gravity="center_vertical"
		        android:background="@drawable/shape_gray_rect">
				<ProgressBar 
				    android:id="@+id/pb_qizhi_three"
			        android:layout_width="match_parent"
			        android:layout_height="7dp"
			        android:progress="50"
			        style="@android:style/Widget.ProgressBar.Horizontal"
			        android:progressDrawable="@drawable/bg_progress_bar"/>	        
		    </LinearLayout>
			<TextView 
			    android:id="@+id/tv_point_three"
			    style="@style/text_gray16_style"
			    android:textSize="14sp"/>
		</LinearLayout>
		<LinearLayout 
		    android:layout_width="wrap_content"
		    android:layout_height="20dp"
		    android:layout_marginTop="10dp"
		    android:gravity="center_vertical">
		    <TextView 
		        style="@style/text_gray16_style"
		        android:textSize="14sp"
		        android:text="@string/qizhi_four"/>
		    <LinearLayout 
		        android:layout_width="200dp"
		        android:layout_height="match_parent"
		        android:layout_marginLeft="5dp"
		        android:gravity="center_vertical"
		        android:background="@drawable/shape_gray_rect">
				<ProgressBar 
				    android:id="@+id/pb_qizhi_four"
			        android:layout_width="match_parent"
			        android:layout_height="7dp"
			        android:progress="50"
			        style="@android:style/Widget.ProgressBar.Horizontal"
			        android:progressDrawable="@drawable/bg_progress_bar"/>	        
		    </LinearLayout>
			<TextView 
			    android:id="@+id/tv_point_four"
			    style="@style/text_gray16_style"
			    android:textSize="14sp"/>
		</LinearLayout>
		<TextView 
	        android:id="@+id/tv_qizhi"
	        android:layout_marginTop="5dp"
        	android:layout_marginBottom="5dp"
	        style="@style/text_black16_style"/>    
	</LinearLayout>
	<TextView 
        android:id="@+id/tv_empty_qizhi"
        style="@style/text_black18_style"
        android:text="@string/empty_qizhi"
        android:visibility="gone"/>
	<View 
	    android:layout_width="match_parent"
	    android:layout_height="10dp"
	    android:background="@color/v2_divider"/>
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="40dp"
	    android:orientation="horizontal">
	    <TextView 
	        android:id="@+id/tv_look_info"
	        android:layout_width="0dp"
	        android:layout_weight="1"
	        android:layout_height="match_parent"
	        android:textColor="#999999"
	        android:textSize="18sp"
	        android:text="@string/look_info"
	        android:gravity="center"/>
	    <ImageView 
	        android:layout_width="1dp"
	        android:layout_height="match_parent"
	        android:background="@color/divider"/>
	    <TextView 
	        android:id="@+id/tv_test"
	        android:layout_width="0dp"
	        android:layout_weight="1"
	        android:layout_height="match_parent"
	        android:textColor="#999999"
	        android:textSize="18sp"
	        android:text="@string/retry_test"
	        android:gravity="center"/>
	</LinearLayout>
	<View 
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:background="@color/v2_divider"/>
</LinearLayout>
