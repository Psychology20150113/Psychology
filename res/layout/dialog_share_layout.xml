<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:orientation="vertical" >
    <ImageView 
        android:id="@+id/iv_finish"
      	style="@style/share_iv_style"
        android:layout_marginTop="30dp"
        android:layout_marginRight="19dp"
        android:layout_alignParentRight="true"
        android:background="@drawable/bg_finish"/>
    <ImageView 
        android:id="@+id/iv_sina"
        style="@style/share_iv_style"
        android:layout_marginTop="118dp"
        android:layout_below="@id/iv_finish"
        android:layout_centerInParent="true" 
        android:background="@drawable/bg_sina"
        />
    <ImageView 
        android:id="@+id/iv_friend"
        style="@style/share_iv_style"
        android:layout_marginTop="25dp"
        android:layout_below="@id/iv_sina"
        android:layout_centerInParent="true" 
        android:background="@drawable/bg_friend"
        />
    <ImageView 
        android:id="@+id/iv_weixin"
        style="@style/share_iv_style"
        android:layout_marginTop="25dp"
        android:layout_below="@id/iv_friend"
        android:layout_centerInParent="true" 
        android:background="@drawable/bg_weixin"
        />
    <ImageView 
        android:id="@+id/iv_space"
        style="@style/share_iv_style"
        android:layout_marginTop="25dp"
        android:layout_below="@id/iv_weixin"
        android:layout_centerInParent="true" 
        android:background="@drawable/bg_space"
        />

</RelativeLayout>
